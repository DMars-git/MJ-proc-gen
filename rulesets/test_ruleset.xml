<ruleset name="testrules" type="series" repeat="f" limit="1">
	<rule name="groundseed" type="basic" rotx="t" roty="t" rotz="f" refx="t" refy="t" refz="f" limit="1">O=T</rule>
	<rule name="groundroot" type="basic" rotx="t" roty="t" rotz="f" refx="t" refy="t" refz="f" limit="-1">O/T=T/T</rule>
	<ruleset name="spread" type="series" repeat="t" limit="-1">
		<rule name="groundspread" type="parallel" rotx="f" roty="t" rotz="f" refx="t" refy="t" refz="f" limit="-1">*,*,*;*,T,*;*,*,*=T,T,T;T,T,T;T,T,T</rule>
	</ruleset>
	<rule name="groundfinish" type="parallel" rotx="f" roty="t" rotz="f" refx="t" refy="t" refz="f" limit="1">T=G</rule>
	<rule name="treeseed" type="basic" rotx="f" roty="t" rotz="f" refx="t" refy="t" refz="f" limit="15">G/O=G/S</rule>
	<rule name="treetrunk" type="basic" rotx="f" roty="t" rotz="f" refx="t" refy="t" refz="f" limit="60">S/O=S/S</rule>
	<rule name="treeleaf" type="parallel" rotx="f" roty="t" rotz="f" refx="t" refy="t" refz="f" limit="1">*,*,*;*,S,*;*,*,*/*,*,*;*,O,*;*,*,*=L,L,L;L,S,L;L,L,L/L,L,L;L,L,L;L,L,L</rule>
</ruleset>
