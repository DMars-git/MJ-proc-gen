	</ruleset>
	<rule name="treeseed" type="basic" rotx="f" roty="f" rotz="f" refx="f" refy="f" refz="f" limit="-1">G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G/O,O,O,O,O,O,O;O,O,O,O,O,O,O;O,O,O,O,O,O,O;O,O,O,O,O,O,O;O,O,O,O,O,O,O;O,O,O,O,O,O,O;O,O,O,O,O,O,O=G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G;G,G,G,G,G,G,G/O,O,O,O,O,O,O;O,O,O,O,O,O,O;O,O,O,O,O,O,O;O,O,O,S,O,O,O;O,O,O,O,O,O,O;O,O,O,O,O,O,O;O,O,O,O,O,O,O</rule>
	<rule name="treetrunk" type="basic" rotx="f" roty="f" rotz="f" refx="f" refy="f" refz="f" limit="120">S/O=S/S</rule>
	<rule name="treeleaf" type="parallel" rotx="f" roty="f" rotz="f" refx="f" refy="f" refz="f" limit="1">*,*,*;*,S,*;*,*,*/*,*,*;*,O,*;*,*,*=L,L,L;L,S,L;L,L,L/L,L,L;L,L,L;L,L,L</rule>
</ruleset>